{
  "type": "object",
  "required": [
    "id",
    "name",
    "validTill",
    "typeCode",
    "enabled"
  ],
  "properties": {
    "id": {
      "type": "integer",
      "example": 123
    },
    "name": {
      "type": "string",
      "example": "Name"
    },
    "description": {
      "type": "string",
      "example": "description"
    },
    "validTill": {
      "type": "string",
      "format": "date",
      "example": "2015-09-09"
    },
    "typeCode": {
      "type": "string",
      "pattern": "^FF(E|D)$",
      "example": "FFE"
    },
    "enabled": {
      "type": "boolean",
      "example": true
    }
  }
}
